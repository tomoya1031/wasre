<%- if controller_name != 'sessions' %>
  <%= link_to "ログインへ", new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規会員登録へ", new_registration_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <!--<%= link_to 'Googleで登録', user_google_oauth2_omniauth_authorize_path, method: :post %>
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", user_google_oauth2_omniauth_callback_path %><br />
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", user_google_oauth2_omniauth_callback_path(resource_name, provider) %><br />
    <%= link_to "Sign in with", user_google_oauth2_omniauth_authorize_path, method: :post %><br />-->
  <% end %>
<% end %>